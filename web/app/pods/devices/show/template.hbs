{{link-to 'Devices' 'devices' class='btn btn-success'}}

<div class="panel panel-default">
<div class="panel-heading">
  <h3 class="panel-title">Sessions for device</h3>
</div>
<div class="panel-body">
  <table class="table  table-hover">
    <thead>
      <tr>
        <th>Id</th>
        <th>Start at</th>
        <th>End at</th>
      </tr>
      </thead>
    <tbody>
      {{#each session in model.sessions}}
      <tr>
        <td>{{session.id}}</td>
        <td {{action 'showEvents' session}} style="cursor:pointer">{{session.start_at}}</td>
        <td {{action 'showEvents' session}} style="cursor:pointer">{{session.end_at}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>

</div>

</div>



