<div class="panel panel-default">
<div class="panel-heading">
  <h3 class="panel-title">Your devices</h3>
</div>
<div class="panel-body">
  <table class="table  table-hover">
    <thead>
      <tr>
        <th>ID</th>
      </tr>
      </thead>
    <tbody>
      {{#each device in model}}
      <tr>
        <td {{action 'showSessions' device}} style="cursor:pointer;vertical-align:middle">{{device.id}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>

</div>

</div>

