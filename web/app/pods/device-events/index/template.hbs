<div id="chart"></div>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">Events for session</h3>
  </div>

  <div class="panel-body">
    <table class="table  table-hover">
      <thead>
        <tr>
          <th>Id</th>
          <th>Local Time</th>
          <th>Lat/Long</th>
          <th>Created at</th>
        </tr>
      </thead>
      <tbody>
        {{#each evt in model}}
        <tr>
          <td>{{evt.id}}</td>
          <td>{{evt.local_time}}</td>
          <td>{{evt.latlng}}</td>
          <td>{{format-date evt.created_at}}</td>
        </tr>
        {{/each}}
      </tbody>
    </table>
  </div>
</div>
