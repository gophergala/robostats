<div class="panel panel-default">
<div class="panel-heading">
  <h3 class="panel-title">Events for session</h3>
</div>
<div class="panel-body">
  <table class="table  table-hover">
    <tbody>
      {{#each event in model.events}}
      <tr>
        <td>{{session.id}}</td>
        <td {{action 'showEvents' session}} style="cursor:pointer">{{session.start_at}}</td>
        <td {{action 'showEvents' session}} style="cursor:pointer">{{session.end_at}}</td>
      </tr>
      {{/each}}
    </tbody>
  </table>
</div>
</div>
